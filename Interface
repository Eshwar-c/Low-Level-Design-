interface books{
    void displayBook();
    default void  book(){
     System.out.println("default book section ");
        }}
class engBook implements books{
    public void displayBook(){
        System.out.println("English Book");
    }
}
class Main{
    public static  void main(String args[]){
        engBook b =  new engBook();
        b.displayBook();
        b.book();
    }
    }
